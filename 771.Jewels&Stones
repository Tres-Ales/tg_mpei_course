//https://leetcode.com/problems/jewels-and-stones/

class Solution {
public:
    int numJewelsInStones(string J, string S) {
        int k=0;
        for (int i=0;i<size(J);i++){
            for (int j=0;j<size(S);j++){
                if(J[i]==S[j]) k++;
            }
        }
     return k;   
    }
};
